{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DevDataFactory401"
		},
		"AzureKeyVault_SinkDB_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://DevKeyVault401.vault.azure.net/"
		},
		"AzureKeyVault_SourceDB_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://DevKeyVault401.vault.azure.net/"
		},
		"StorageAzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://DevKeyVault401.vault.azure.net/"
		},
		"lnk_azureblob_input_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "StorageKeyVault"
		},
		"lnk_azuresql_destinationdb_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "KVDestinationDB"
		},
		"lnk_azuresql_sourcedb_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "KVSourceDB"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DevDataFactory401_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureKeyVault_SinkDB_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_SinkDB_properties_typeProperties_baseUrl')]"
					},
					"AzureKeyVault_SourceDB_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_SourceDB_properties_typeProperties_baseUrl')]"
					},
					"StorageAzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('StorageAzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"lnk_azureblob_input_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('lnk_azureblob_input_properties_typeProperties_connectionString_secretName')]"
					},
					"lnk_azuresql_destinationdb_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('lnk_azuresql_destinationdb_properties_typeProperties_connectionString_secretName')]"
					},
					"lnk_azuresql_sourcedb_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('lnk_azuresql_sourcedb_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}