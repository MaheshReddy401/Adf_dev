{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFDevCovid"
		},
		"ls_adl_population_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adl_population'"
		},
		"ls_blb_population_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blb_population'"
		},
		"AzureKeyVault_SinkDB_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://DevKeyVault401.vault.azure.net/"
		},
		"AzureKeyVault_SourceDB_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://DevKeyVault401.vault.azure.net/"
		},
		"StorageAzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://DevKeyVault401.vault.azure.net/"
		},
		"lnk_azureblob_input_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "StorageKeyVault"
		},
		"lnk_azuresql_destinationdb_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "KVDestinationDB"
		},
		"lnk_azuresql_sourcedb_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "KVSourceDB"
		},
		"ls_adl_population_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adldevcovidstorage.dfs.core.windows.net"
		},
		"se_copy_tr_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/70c7b9d7-2741-4798-8b55-6caa956c994d/resourceGroups/RG-Dev/providers/Microsoft.Storage/storageAccounts/blbdevstorage"
		},
		"ls_ecdc_covid_deaths_http_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFDevCovid_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adl_population_accountKey": {
						"value": "[parameters('ls_adl_population_accountKey')]"
					},
					"ls_blb_population_connectionString": {
						"value": "[parameters('ls_blb_population_connectionString')]"
					},
					"AzureKeyVault_SinkDB_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_SinkDB_properties_typeProperties_baseUrl')]"
					},
					"AzureKeyVault_SourceDB_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_SourceDB_properties_typeProperties_baseUrl')]"
					},
					"StorageAzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('StorageAzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"ls_adl_population_properties_typeProperties_url": {
						"value": "[parameters('ls_adl_population_properties_typeProperties_url')]"
					},
					"ls_ecdc_covid_deaths_http_properties_typeProperties_url": {
						"value": "[parameters('ls_ecdc_covid_deaths_http_properties_typeProperties_url')]"
					},
					"lnk_azureblob_input_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('lnk_azureblob_input_properties_typeProperties_connectionString_secretName')]"
					},
					"lnk_azuresql_destinationdb_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('lnk_azuresql_destinationdb_properties_typeProperties_connectionString_secretName')]"
					},
					"lnk_azuresql_sourcedb_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('lnk_azuresql_sourcedb_properties_typeProperties_connectionString_secretName')]"
					},
					"se_copy_tr_properties_typeProperties_scope": {
						"value": "[parameters('se_copy_tr_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}